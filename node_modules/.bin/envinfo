#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/reactjs/sessions/Components-and-Props/node_modules/.pnpm/envinfo@7.11.0/node_modules/envinfo/dist/cli.js/node_modules:/home/nxtwave/reactjs/sessions/Components-and-Props/node_modules/.pnpm/envinfo@7.11.0/node_modules/envinfo/dist/node_modules:/home/nxtwave/reactjs/sessions/Components-and-Props/node_modules/.pnpm/envinfo@7.11.0/node_modules/envinfo/node_modules:/home/nxtwave/reactjs/sessions/Components-and-Props/node_modules/.pnpm/envinfo@7.11.0/node_modules:/home/nxtwave/reactjs/sessions/Components-and-Props/node_modules/.pnpm/node_modules:/home/nxtwave/reactjs/sessions/Components-and-Props/node_modules:/home/nxtwave/reactjs/sessions/node_modules:/home/nxtwave/reactjs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/reactjs/sessions/Components-and-Props/node_modules/envinfo/dist/cli.js/node_modules:/home/nxtwave/reactjs/sessions/Components-and-Props/node_modules/envinfo/dist/node_modules:/home/nxtwave/reactjs/sessions/Components-and-Props/node_modules/envinfo/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../envinfo/dist/cli.js" "$@"
else 
  exec node  "$basedir/../envinfo/dist/cli.js" "$@"
fi
