#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/reactjs/sessions/Components-and-Props/node_modules/.pnpm/rimraf@2.7.1/node_modules/rimraf/bin.js/node_modules:/home/nxtwave/reactjs/sessions/Components-and-Props/node_modules/.pnpm/rimraf@2.7.1/node_modules/rimraf/node_modules:/home/nxtwave/reactjs/sessions/Components-and-Props/node_modules/.pnpm/rimraf@2.7.1/node_modules:/home/nxtwave/reactjs/sessions/Components-and-Props/node_modules/.pnpm/node_modules:/home/nxtwave/reactjs/sessions/Components-and-Props/node_modules:/home/nxtwave/reactjs/sessions/node_modules:/home/nxtwave/reactjs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/reactjs/sessions/Components-and-Props/node_modules/rimraf/bin.js/node_modules:/home/nxtwave/reactjs/sessions/Components-and-Props/node_modules/rimraf/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../rimraf/bin.js" "$@"
else 
  exec node  "$basedir/../rimraf/bin.js" "$@"
fi
